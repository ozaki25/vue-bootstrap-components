{
  "name": "vue-storybook-sample",
  "version": "0.1.0",
  "private": true,
  "main": "dist/my-component-min.js",
  "scripts": {
    "build": "rollup -c build/rollup.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "npm run build && build-storybook && cd storybook-static && git init && git add -A && git commit -m 'deploy storybook' && git push -f git@github.com:ozaki25/vue-storybook-sample.git master:gh-pages"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/vue": "^5.1.9",
    "@vue/babel-preset-app": "^3.8.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "core-js": "^2.6.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.12.0",
    "prettier-stylelint": "^0.4.2",
    "rollup": "^1.16.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^7.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.8.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  }
}
